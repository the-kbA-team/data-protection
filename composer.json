  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/the-kbA-team/data-protection/issues"
  },
  "keywords": [
    "oneway",
    "encrypted",
    "deterministic",
    "encryption",
    "searchable",
    "search",
    "social security number",
    "ssn",
    "data protection"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
